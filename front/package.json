{
  "name": "taiga-contrib-gogs",
  "version": "1.9.1",
  "description": "Taiga contrib plugin for gogs integration",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist || true",
    "create-dist": "mkdir -p dist",
    "copy-statics": "cp gogs.json dist/",
    "build-js": "{ cat 'coffee/gogs.coffee'; node_modules/jade/bin/jade.js < 'partials/gogs.jade'; } | node_modules/coffee-script/bin/coffee --compile --stdio > dist/gogs.js",
    "build": "npm run clean; npm run create-dist; npm run copy-statics; npm run build-js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taigaio/taiga-contrib-gogs"
  },
  "keywords": [
    "gogs",
    "taiga",
    "integration"
  ],
  "author": "Jes√∫s Espino <jesus.espino@kaleidos.net>",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/taigaio/taiga-contrib-gogs/issues"
  },
  "homepage": "https://github.com/taigaio/taiga-contrib-gogs",
  "dependencies": {
    "coffee-script": "^1.8.0",
    "jade": "^1.9.1"
  }
}
